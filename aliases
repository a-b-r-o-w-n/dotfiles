# Unix
alias tlf="tail -f"
alias ln='ln -v'
alias mkdir='mkdir -p'
alias ...='../..'
alias l='ls'
alias ll='ls -al'
alias lh='ls -Alh'
alias -g G='| grep'
alias -g M='| less'
alias -g L='| wc -l'
alias -g ONE="| awk '{ print \$1}'"
alias emacs="/usr/local/Cellar/emacs/24.5/bin/emacs"
alias vim="$EDITOR"
alias e="$EDITOR"
alias v="$VISUAL"

# git
alias g='git'
alias grh='g f && g res --hard HEAD@{u}'
alias gup='g f && g rebase'

# General
alias k=clear
